{

  "manifest_version": 2,
  "name": "sbore",
  "version": "1.0",

  "description": "collects links on web articles for easy post-reading access.",
  "content_scripts": [
  	{
      "matches": ["*://*/*"],
  	"js": ["sbore.js"]
  }
  ],
  "browser_action": {
    "default_icon": {"16": "icons/16.png"},
    "default_popup": "popup/list.html"},
  "application": {
  	"gecko": {
  		"id": "sbore@github.io"
  	}
  }
}
